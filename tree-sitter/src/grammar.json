{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "mealplan",
  "rules": {
    "source_file": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "persons_line"
        },
        {
          "type": "STRING",
          "value": "\n"
        },
        {
          "type": "SYMBOL",
          "name": "starttag_line"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "\n"
              },
              {
                "type": "SYMBOL",
                "name": "day_line"
              }
            ]
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "\n"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "persons_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "Personen:"
        },
        {
          "type": "FIELD",
          "name": "count",
          "content": {
            "type": "SYMBOL",
            "name": "integer"
          }
        }
      ]
    },
    "starttag_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "Starttag:"
        },
        {
          "type": "FIELD",
          "name": "date",
          "content": {
            "type": "SYMBOL",
            "name": "date"
          }
        }
      ]
    },
    "integer": {
      "type": "PATTERN",
      "value": "\\d+"
    },
    "date": {
      "type": "PATTERN",
      "value": "\\d{4}-\\d{2}-\\d{2}"
    },
    "day_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "day",
          "content": {
            "type": "SYMBOL",
            "name": "day_with_count"
          }
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "menu"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "day_with_count": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "day_name"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "count",
              "content": {
                "type": "SYMBOL",
                "name": "count"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "day_name": {
      "type": "PATTERN",
      "value": "[^:\\n(]+"
    },
    "count": {
      "type": "PATTERN",
      "value": "\\(\\d+\\)"
    },
    "menu": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "rest_day"
        },
        {
          "type": "SYMBOL",
          "name": "menu_items"
        }
      ]
    },
    "rest_day": {
      "type": "STRING",
      "value": "Reste"
    },
    "menu_items": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "menu_item"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ","
              },
              {
                "type": "SYMBOL",
                "name": "menu_item"
              }
            ]
          }
        }
      ]
    },
    "menu_item": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "dish_with_count"
        },
        {
          "type": "SYMBOL",
          "name": "shopping_marker"
        }
      ]
    },
    "shopping_marker": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "⟨"
        },
        {
          "type": "PATTERN",
          "value": "[^⟩]+"
        },
        {
          "type": "STRING",
          "value": "⟩"
        }
      ]
    },
    "dish_with_count": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "dish",
          "content": {
            "type": "SYMBOL",
            "name": "dish"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "count",
              "content": {
                "type": "SYMBOL",
                "name": "count"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "dish": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "[["
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "PATTERN",
            "value": "[^\\]]+"
          }
        },
        {
          "type": "STRING",
          "value": "]]"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "[ \\t]"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}